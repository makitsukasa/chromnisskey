{
	"name": "chromnisskey",
	"version": "1.0.0",
	"manifest_version": 3,
	"description": "chromnisskey[chrome + omni + misskey] note misskey from omni bar",
	"permissions":[
		"tabs",
		"notifications"
	],
	"host_permissions":[
	],
	"content_scripts": [{
		"matches": ["https://twitter.com/compose/tweet?chromnisskey=true&text=*"],
		"js": ["twitter_autopost.js"]
	}],
	"background": {
		"service_worker":"service_worker.js"
	},
	"omnibox": {
		"keyword": "tw"
	}
}
